{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DatabaseConfiguration": {
    "CommandTimeoutSeconds": 30,
    "ConnectionStrings": {
      "WolverineDB": "Data Source={$MSSQL_DATA_SOURCE};Initial Catalog={$MSSQL_CATALOG};Integrated Security=True"
    },
    "StoredProcedureNames": {
      "WeatherForecastInsert": "{$WeatherForecastInsert}",
      "WeatherForecastGetById": "{$WeatherForecastGetById}",
      "WeatherForecastGetAll": "{$WeatherForecastGetAll}"
    }
  }
}
